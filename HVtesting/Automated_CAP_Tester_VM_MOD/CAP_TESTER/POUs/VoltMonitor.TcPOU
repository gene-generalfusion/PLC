  <POU Name="VoltMonitor" Id="{ab398dd9-3a48-4aac-bec4-7b546cd8c4a7}" SpecialFunc="None">
    <Declaration><![CDATA[PROGRAM VoltMonitor
VAR
	i : USINT := 0;
	b : ARRAY[0..3] OF BOOL;
	b1 : BOOL;
	b2 : BOOL;
	b3 : BOOL;
	pause : BOOL;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[IF Heartbeat.screen_refresh_pulse = TRUE AND pause = FALSE THEN
	i := i+1;
	VoltMonitor_DB.Channel := i ;
	VoltMonitor_DB.VoltageReading_raw[i] := GVL.raw_VoltageMonitor;
END_IF

b[0] := VoltMonitor_DB.Channel.0;
b[1] := VoltMonitor_DB.Channel.1;
b[2] := VoltMonitor_DB.Channel.2;
b[3] := VoltMonitor_DB.Channel.3;
	
IF i > 15 THEN 
	i := 0;
END_IF]]></ST>
    </Implementation>
