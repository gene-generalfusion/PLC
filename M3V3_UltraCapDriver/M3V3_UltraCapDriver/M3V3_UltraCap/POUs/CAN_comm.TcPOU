	MCP_IO.MagDriver[1].Cap[1].CanTx_config_request[0] := 16#04;
	MCP_IO.MagDriver[1].Cap[1].CanTx_config_request[1] := 16#27;
	MCP_IO.MagDriver[1].Cap[1].CanTx_config_request[2] := 16#02;
