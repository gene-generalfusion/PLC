	MCP_IO.MagDriver[1].Cap[1].CanTx_config_request[4] := 16#9A;
	(*Output.R2_06_CANrequest.cobId := 16#0CDA7FCC;
	Output.R2_06_CANrequest.length := 5;
	Output.R2_06_CANrequest.data[0] := 16#04;
